<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:WPF_lab_7_8.Dictionaries">

    <Style x:Key="Background">
        <Setter Property="Control.Background" Value="Bisque" />
    </Style>

    <Style x:Key="Foreground">
        <Setter Property="Control.Foreground" Value="Violet" />
    </Style>

    <Style x:Key="1" TargetType="Button">

        <Setter Property="Foreground" Value="Pink" />

    </Style>

</ResourceDictionary>